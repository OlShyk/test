{ "root": true,
  "extends": [ 
    "eslint:recommended",
    "prettier"
], 
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "indent": 1,
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint", 
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13, 
        "sourceType": "module" 
    },
    "plugins": [
        "prettier"
    ]
}